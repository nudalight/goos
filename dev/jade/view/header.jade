md-toolbar()
    .md-toolbar-tools(
        layout='row'
        layout-align='end center'
    )

        md-button(
            href='#/login'
            ng-show='!user.token'
        ) Login

        md-button(
            href='#/register'
            ng-show='!user.token'
        ) Register

        md-button(
            ng-click='actions.logout()'
            ng-show='user.token'
        ) Log out